[user]
    name = Matt Styles
    email = mstyles@insidesales.com
[push]
    default = upstream
[log]
    decorate = auto
[color]
    ui = auto
[status]
    short = true
    branch = true
[core]
    excludesfile = ~/.gitignore
    editor = vim
[merge]
    tool = vimdiff
[alias]
    lint = !git diff --cached --name-only --diff-filter=AM | grep 'php' | xargs -P 10 -n1 php -l
    recent = "!git reflog | egrep -io \"moving from ([^[:space:]]+)\" | awk '{ print $3 }' | awk ' !x[$0]++' | head"
